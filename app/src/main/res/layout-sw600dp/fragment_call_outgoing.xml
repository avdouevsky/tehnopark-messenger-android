<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_dark_blue_to_blue"
    tools:ignore="MissingPrefix">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="30dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="right"
            android:layout_marginTop="3.5dp">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_50"
                android:layout_marginLeft="10dp"
                android:id="@+id/fragment_call_outgoing_fl_dot_one"/>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_20"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"
                android:id="@+id/fragment_call_outgoing_fl_dot_two"/>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_10"
                android:id="@+id/fragment_call_outgoing_fl_dot_three"/>
        </LinearLayout>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/incoming_call_title"
            android:textColor="@color/colorWhiteText"
            android:textSize="15sp"
            android:layout_weight="1"
            android:gravity="center"
            android:letterSpacing="0.09"
            fontPath="fonts/GothamProBold.ttf"
            />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_weight="1"
            android:gravity="left"
            android:layout_marginTop="3.5dp">

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_10"
                android:id="@+id/fragment_call_outgoing_fl_dot_four"/>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_20"
                android:layout_marginLeft="9dp"
                android:layout_marginRight="9dp"
                android:id="@+id/fragment_call_outgoing_fl_dot_five"/>

            <FrameLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/bleeping_dot_50"
                android:layout_marginRight="10dp"
                android:id="@+id/fragment_call_outgoing_fl_dot_six"/>

        </LinearLayout>

    </LinearLayout>


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/outgoing_call_title_call_type"
        android:textColor="@color/colorWhiteText"
        android:textSize="15sp"
        android:letterSpacing="0.09"
        fontPath="fonts/GothamProBold.ttf"
        android:layout_gravity="center"
        android:layout_marginTop="10dp"
        />


    <FrameLayout
        android:layout_width="204dp"
        android:layout_height="204dp"
        android:layout_gravity="center"
        android:background="@drawable/profile_icon_circle_cover_inc"
        android:layout_marginTop="44dp">

        <ImageView
            android:layout_width="183dp"
            android:layout_height="183dp"
            android:layout_gravity="center"
            android:id="@+id/fragment_call_outgoing_image_picture" />
    </FrameLayout>



    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/incoming_call_profile_name"
        android:textColor="@color/colorWhiteText"
        android:layout_gravity="center"
        android:gravity="center"
        android:layout_marginTop="27dp"
        android:minLines="2"
        android:textSize="36sp"
        fontPath="fonts/GothamPro.ttf"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/incoming_call_department"
        android:textColor="@color/colorWhiteText"
        android:textSize="15sp"
        android:letterSpacing="0.09"
        android:layout_marginTop="16dp"
        fontPath="fonts/GothamPro.ttf"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:text="@string/incoming_call_position"
        android:textColor="@color/colorWhiteText"
        android:textSize="15sp"
        android:letterSpacing="0.09"
        android:layout_marginTop="10dp"
        fontPath="fonts/GothamPro.ttf"/>

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="01">
    </FrameLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_marginTop="42dp">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginLeft="18dp">

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="105dp"
                android:background="@drawable/call_button_background"
                android:layout_gravity="right"
                android:id="@+id/fragment_call_outgoing_fl_loudspeaker">

                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="24dp"
                    android:src="@drawable/loudspeaker"
                    android:id="@+id/fragment_call_outgoing_image_loudspeaker"
                    android:layout_gravity="center"
                    />

            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/incoming_call_first_icon"
                android:textColor="@color/colorWhiteText"
                android:textSize="15sp"
                android:letterSpacing="0.09"
                android:layout_gravity="right"
                android:layout_marginTop="18dp"
                android:layout_marginRight="15dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1">

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="105dp"
                android:background="@drawable/call_button_background"
                android:layout_gravity="center"
                android:id="@+id/fragment_call_outgoing_fl_message">

                <ImageView
                    android:layout_width="27dp"
                    android:layout_height="27dp"
                    android:src="@drawable/message"
                    android:layout_gravity="center"/>

            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/incoming_call_second_icon"
                android:textColor="@color/colorWhiteText"
                android:textSize="15sp"
                android:letterSpacing="0.09"
                android:layout_gravity="center"
                android:layout_marginTop="18dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginRight="18dp">

            <FrameLayout
                android:layout_width="105dp"
                android:layout_height="105dp"
                android:background="@drawable/call_button_background"
                android:layout_gravity="left"
                android:id="@+id/fragment_call_outgoing_fl_mute">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="30dp"
                    android:src="@drawable/mute"
                    android:id="@+id/fragment_call_outgoing_image_mute"
                    android:layout_gravity="center"/>

            </FrameLayout>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/fragment_call_outgoing_tv_mute"
                android:text="@string/incoming_call_third_icon"
                android:textColor="@color/colorWhiteText"
                android:textSize="15sp"
                android:letterSpacing="0.09"
                android:layout_gravity="left"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="18dp"/>

        </LinearLayout>

    </LinearLayout>

    <FrameLayout
        android:layout_width="105dp"
        android:layout_height="105dp"
        android:background="@drawable/call_button_background_red"
        android:layout_gravity="center"
        android:layout_marginTop="18dp"
        android:id="@+id/fragment_call_outgoing_fl_call_end">

        <ImageView
            android:layout_width="42dp"
            android:layout_height="15dp"
            android:src="@drawable/end_call"
            android:layout_gravity="center"/>

    </FrameLayout>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/incoming_call_end_icon"
        android:textColor="@color/colorWhiteText"
        android:textSize="15sp"
        android:letterSpacing="0.09"
        android:layout_gravity="center"
        android:layout_marginTop="18dp"
        android:layout_marginBottom="30sp"/>


</LinearLayout>