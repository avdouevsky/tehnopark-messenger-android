<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <attr name="face_type" format="enum">
            <enum name="BLACK" value="0"/>
            <enum name="BLACK_ITALIC" value="1"/>
            <enum name="REGULAR_NORMAL" value="2"/>
            <enum name="REGULAR_BOLD" value="3"/>
            <enum name="REGULAR_BOLD_ITALIC" value="4"/>
            <enum name="REGULAR_ITALIC" value="5"/>
            <enum name="LIGHT_NORMAL" value="6"/>
            <enum name="LIGHT_ITALIC" value="7"/>
            <enum name="MEDIUM_NORMAL" value="8"/>
            <enum name="MEDIUM_ITALIC" value="9"/>
            <enum name="NARROW_MEDIUM" value="10"/>
            <enum name="NARROW_BOLD" value="11"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="PinBoardView">
        <attr name="pin_routine">
            <enum name="SETUP" value="0"/>
            <enum name="INPUT" value="1"/>
        </attr>
    </declare-styleable>
</resources>